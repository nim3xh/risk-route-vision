# Google Maps API Key (required for map display)
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# API Base URL (leave empty to use mock mode)
VITE_API_BASE=

# Timezone (default: Asia/Colombo)
VITE_TIMEZONE=Asia/Colombo

# Use mock API (true|false) - automatically true if API_BASE is empty
VITE_USE_MOCK_API=true
